<p id="notice"><%= notice %></p>

<h1>Listing Buyers</h1>

<table>
  <thead>
    <tr>
      <th>Buyer name</th>
      <th>Phone mobile</th>
      <th>Phone home</th>
      <th>Phone work</th>
      <th>Phone fax</th>
      <th>Email</th>
      <th>Employer</th>
      <th>Search time</th>
      <th>Other agent</th>
      <th>Address</th>
      <th>Family size</th>
      <th>Own or rent</th>
      <th>Current mortgage</th>
      <th>Need sell</th>
      <th>Lease length</th>
      <th>Really liked</th>
      <th>Why not</th>
      <th>Move time</th>
      <th>Why move time</th>
      <th>Price range</th>
      <th>Qualified</th>
      <th>Lender</th>
      <th>Downpayment</th>
      <th>Monthly budget</th>
      <th>Bedrooms</th>
      <th>Square feet</th>
      <th>Units</th>
      <th>Location</th>
      <th>Style</th>
      <th>Decision</th>
      <th>Helper</th>
      <th>Requirements</th>
      <th>Use as agent</th>
      <th>Times</th>
      <th>Communication</th>
      <th>Best time</th>
      <th>Sites</th>
      <th>Other information</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buyers.each do |buyer| %>
      <tr>
        <td><%= buyer.buyer_name %></td>
        <td><%= buyer.phone_mobile %></td>
        <td><%= buyer.phone_home %></td>
        <td><%= buyer.phone_work %></td>
        <td><%= buyer.phone_fax %></td>
        <td><%= buyer.email %></td>
        <td><%= buyer.employer %></td>
        <td><%= buyer.search_time %></td>
        <td><%= buyer.other_agent %></td>
        <td><%= buyer.address %></td>
        <td><%= buyer.family_size %></td>
        <td><%= buyer.own_or_rent %></td>
        <td><%= buyer.current_mortgage %></td>
        <td><%= buyer.need_sell %></td>
        <td><%= buyer.lease_length %></td>
        <td><%= buyer.really_liked %></td>
        <td><%= buyer.why_not %></td>
        <td><%= buyer.move_time %></td>
        <td><%= buyer.why_move_time %></td>
        <td><%= buyer.price_range %></td>
        <td><%= buyer.qualified %></td>
        <td><%= buyer.lender %></td>
        <td><%= buyer.downpayment %></td>
        <td><%= buyer.monthly_budget %></td>
        <td><%= buyer.bedrooms %></td>
        <td><%= buyer.square_feet %></td>
        <td><%= buyer.units %></td>
        <td><%= buyer.location %></td>
        <td><%= buyer.style %></td>
        <td><%= buyer.decision %></td>
        <td><%= buyer.helper %></td>
        <td><%= buyer.requirements %></td>
        <td><%= buyer.use_as_agent %></td>
        <td><%= buyer.times %></td>
        <td><%= buyer.communication %></td>
        <td><%= buyer.best_time %></td>
        <td><%= buyer.sites %></td>
        <td><%= buyer.other_information %></td>
        <td><%= link_to 'Show', buyer %></td>
        <td><%= link_to 'Edit', edit_buyer_path(buyer) %></td>
        <td><%= link_to 'Destroy', buyer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

